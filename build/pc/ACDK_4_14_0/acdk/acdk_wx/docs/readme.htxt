      <perleval> return acdk::getDependencies("./package.xml"); </perleval>
    </quoted>
  <jump2>Namespaces</jump2>
    <quoted>
      <perleval> return acdk::getnsrefs("./package.xml"); </perleval>
    </quoted>
  <jump2>Authors</jump2>
