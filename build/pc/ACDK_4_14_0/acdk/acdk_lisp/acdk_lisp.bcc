	$(MAKE) -f acdklisp.bcc metainfo
	cd ..


acdk_lisp_Test:  acdk_lisp
	cd tests/acdk/lisp
	$(MAKE) -f acdk_lisp_Test.bcc
	cd ../../..


acdk_lisp_Test-clean:
	cd tests/acdk/lisp
	$(MAKE) -f acdk_lisp_Test.bcc clean
	cd ../../..


acdk_lisp_Test-metainfo:
