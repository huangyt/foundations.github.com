acdk_lisp:  
	cd src
	$(MAKE) -f acdk_lisp.bcc
	cd ..


acdk_lisp-clean:
	cd src
	$(MAKE) -f acdk_lisp.bcc clean
	cd ..


acdk_lisp-metainfo:
	cd src
	$(MAKE) -f acdk_lisp.bcc metainfo
	cd ..


acdklisp:  acdk_lisp
	cd src
	$(MAKE) -f acdklisp.bcc
	cd ..


acdklisp-clean:
	cd src
	$(MAKE) -f acdklisp.bcc clean
	cd ..


acdklisp-metainfo:
